<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/dp_10">


    <ImageView
        android:id="@+id/item_treasures_image"
        android:layout_width="@dimen/dp_130"
        android:layout_height="@dimen/dp_100"
        android:layout_gravity="center"
        android:layout_margin="@dimen/dp_10"
        android:layout_weight="0"
        android:scaleType="fitXY"
        android:src="#eeeeee" />

    <TextView
        android:id="@+id/item_treasures_tv_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/item_treasures_image"
        android:layout_toRightOf="@+id/item_treasures_image"
        android:textColor="@color/color_844949"
        android:textSize="@dimen/size_16"
        tools:text="标题标题标题标题标题" />


    <TextView
        android:id="@+id/item_treasures_tv_leave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_treasures_tv_title"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_toRightOf="@+id/item_treasures_image"
        android:layout_weight="1"
        android:textColor="#5a5263"
        tools:text="保护等级:国家级" />

    <TextView
        android:id="@+id/item_treasures_tv_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_treasures_tv_title"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_toRightOf="@+id/item_treasures_tv_leave"
        android:layout_weight="1"
        android:textColor="@color/color_5a5263"
        tools:text="风险类别:二类" />


    <TextView
        android:id="@+id/item_treasures_tv_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_treasures_tv_type"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_toRightOf="@+id/item_treasures_image"
        android:drawableLeft="@mipmap/ww_address"
        android:textColor="@color/color_5a5263"
        tools:text="北京市海淀区" />


    <TextView
        android:id="@+id/item_treasures_tv_totalrecord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_treasures_image"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_5"
        android:drawableLeft="@mipmap/xj_xx_num_ico"
        android:drawablePadding="@dimen/dp_10"
        android:textColor="#22819f"
        tools:text="巡检10次" />


    <TextView
        android:id="@+id/item_treasures_tv_error_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_treasures_image"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_toRightOf="@+id/item_treasures_tv_totalrecord"
        android:textColor="#bf3636"
        tools:text="异常信息:0" />

    <TextView
        android:id="@+id/item_treasures_tv_file_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_treasures_image"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_toRightOf="@+id/item_treasures_tv_error_num"
        android:textColor="#bf3636"
        tools:text="已归档(5)" />

    <TextView
        android:id="@+id/item_treasures_tv_nofile_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/item_treasures_image"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_toRightOf="@+id/item_treasures_tv_file_num"
        android:textColor="#bf3636"
        tools:text="未归档(5)" />


